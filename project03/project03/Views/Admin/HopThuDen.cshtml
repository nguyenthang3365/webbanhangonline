@model IEnumerable<project03.Models.entiy.LIENHE>
@{
    ViewBag.Title = "Hộp thư đến";
    Layout = "~/Views/Shared/Layoutadmin.cshtml";
}

<div class="col-md-6">
    <div class="card">
        <div class="card-header">
            <h5 class="card-category">Hộp Thư Đến</h5>

        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table">
                  
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>
                                @item.EMAIL

                            </td>
                            <td>
                                @item.TINNHAN
                            </td>
                            <td class="text-right">
                                @item.NGAYGUI.ToString("dd/MM/yyyy")
                            </td>
                        </tr>

                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>




